<div class="iconsize">
	<input class="iconsize-slider" type="slider" v-model="size"/>
	<svg class="iconsize-preview" viewbox="0 0 128 128" :width="currentWidth" :height="currentHeight">
		<use :href="`#icon-${icon}`"/>
	</svg>
	<button class="iconsize-confirm" @click.left="onIconSizeConfirm"/>
</div>